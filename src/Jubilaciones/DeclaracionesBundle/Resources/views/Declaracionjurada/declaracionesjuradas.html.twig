{% extends "@JubilacionesDeclaraciones/layout.html.twig" %}
{% block title %}Declaraciones Juradas{% endblock %}
{% block metadesc %}Se impartiran las charlas: 
    
{% endblock %}
{% block pageid 'eventos' %}

    {% block contenido %}
        {# CONTENIDO ORIGINAL DEL ARCHIVO Y SIN LAS ETIQUETAS <html><body> ï¿½ </body></html> #}

        <h1>Declaraciones Juradas</h1>
        <div>
                <div class="grid_2 alpha">
                    <b>Periodo</b> <b>Tipo</b>
                </div>
                <div class="grid_10 omega">
                    <b>Estado</b>
                </div>
            </div>
        {% for declaracion in declaraciones %}
            <div>
                <div class="grid_2 alpha">
                    {{ declaracion.periodo[0:4] }} / {{ declaracion.periodo[4:2] }}
                    {{ declaracion.tipoLiquidacion }}
                </div>
                <div class="grid_10 omega">
                    {{ declaracion.estado }}
                </div>
            </div>
        {% endfor %}
    {% endblock %}
    {% block lateral %}
        <h3>Falta alg&uacute;n evento?</h3>
        <p>Accede a la secci&oacute;n <a href="#"><em>Call for papers</em></a> y env&iacute;anos tu propuesta</p>
        {{ parent() }}
    {% endblock %}  