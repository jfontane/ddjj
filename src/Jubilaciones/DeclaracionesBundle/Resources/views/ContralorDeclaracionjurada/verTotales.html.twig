    {% block body %}
        {# CONTENIDO ORIGINAL DEL ARCHIVO Y SIN LAS ETIQUETAS <html><body> � </body></html> #}
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="{{ path('principal_logueado') }}">Home</a></li>
                <li class="breadcrumb-item"><a href="{{path('contralor_declaracion_listar_pendientes')}}">Declaraciones Juradas</a></li>
                <li class="breadcrumb-item active" aria-current="page">Ver</li>
            </ol>
        </nav>
        <div class="grid_16">
          <table class="table">
            <thead class="thead-dark">
              <tr>
                <th colspan="2">Datos del Organismos</th>
              </tr>
            </thead>
            <tbody>
            <tr>
              <th>Codigo Organismo</th><td>{{organismo.codigo}}</td>
            </tr>
            <tr>
              <th>Nombre</th><td>{{organismo.nombre}}</td>
            </tr>
            <tr>
              <th>Localidad</th><td>{{organismo.localidad}}</td>
            </tr>
            <tr>
              <th>Periodo</th><td>{{declaracion.periodoAnio}}/{{declaracion.periodoMes}}</td>
            </tr>
            <tr>
              <th>Tipo Liquidacion</th><td>{{declaracion.tipoLiquidacion}}</td>
            </tr>
          </tbody>
          </table>
          <hr>
            <table class="table">
                <thead class="thead-dark">
                    <tr>
                        <th>Concepto</th>
                        <th>Monto ($)</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <th>Remunerativo</th>
                        <td>{{ valores.totalRemunerativo | number_format(2, ',', '.') }}</td>
                    </tr>
                    <tr>
                        <th>No Remunerativo</th>
                        <td>{{ valores.totalNoRemunerativo | number_format(2, ',', '.') }}</td>
                    </tr>
                    <tr>
                        <th>Aporte Personal</th>
                        <td>{{ valores.totalApPersonal | number_format(2, ',', '.') }}</td>
                    </tr>
                    <tr>
                        <th>Contribucion Patronal</th>
                        <td>{{ valores.totalApPatronal | number_format(2, ',', '.') }}</td>
                    </tr>
                    <tr>
                        <th>Inasistencia - Suspensiones</th>
                        <td>{{ valores.totalInasisSusp | number_format(2, ',', '.') }}</td>
                    </tr>
                    <tr>
                        <th>Licencia sin goce de Haberes</th>
                        <td>{{ valores.totalLicSinSueldo | number_format(2, ',', '.') }}</td>
                    </tr>
                    <tr>
                        <th>Tarea Riesgosa</th>
                        <td>{{ valores.totalTareasRiesgoza | number_format(2, ',', '.') }}</td>
                    </tr>
                    <tr>
                        <th>Archivo Jubi.dat</th>
                        <td>
                            <a href="{{path('contralor_jubidat',{'id' : declaracion.id })}}">Jubi.dat</a>
                        </td>
                    </tr>
                    <tr>
                        <th>Archivo Jubi.dat</th>
                        <td>
                            <a href="{{path('contralor_jubi1ind',{'id' : declaracion.id })}}">Jubi1.ind</a>
                        </td>
                    </tr>
                </tbody>
            </table>
            <hr>
            <table>
                <tr>
                    <td>&nbsp;</td>
                    <td colspan='2' align='center'>
                        <a href="{{path('contralor_declaracion_listar_pendientes')}}">Volver</a>&nbsp;&nbsp;
                    </td>
                    <td>&nbsp;</td>
                </tr>

            </table>
            <br>
        </div>
    {% endblock %}
